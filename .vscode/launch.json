    "version": "2.0.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "AppProject",
            "preLaunchTask": "npm: dev",
            "runtimeExecutable": "node",
            "program": "${workspaceFolder}/dist/app/server.js",
            "outFiles": [ "${workspaceFolder}/dist/*.js" ],
            "sourceMaps": true,
            "protocol": "inspector",
            "outputCapture": "std",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
            }
        }
    ]
}
